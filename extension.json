{
	"name": "GetFirst2Sentences",
	"author": "Bilka",
	"url": "https://github.com/Bilka2/GetFirst2Sentences",
	"description": "Magic word that gets the first 2 sentences of a page.",
	"type": "variable",
	"license-name": "GNU GPLv3",
	"version": "0.1.1",
	"Hooks": {
		"ParserGetVariableValueSwitch": [
			"GetFirst2Sentences\\GetFirst2Sentences::getMagicWord"
		],
		"MagicWordwgVariableIDs": [
			"GetFirst2Sentences\\GetFirst2Sentences::registerMagicWord"
		]
	},
	"AutoloadClasses": {
		"GetFirst2Sentences\\GetFirst2Sentences": "GetFirst2Sentences.php"
	},
	"ExtensionMessagesFiles": {
		"GetFirst2SentencesMagic": "GetFirst2Sentences.i18n.magic.php"
	}
}
